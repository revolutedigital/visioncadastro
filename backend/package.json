{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "prisma generate && tsc",
    "start": "node dist/index.js",
    "start:migrate": "prisma migrate deploy && node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration --runInBand",
    "test:performance": "jest --testPathPattern=tests/performance --runInBand --testTimeout=120000",
    "test:regression": "jest --testPathPattern=tests/regression --runInBand",
    "test:coverage": "jest --coverage --coverageDirectory=coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:sprint1": "npx ts-node src/tests/test-sprint1.ts",
    "test:sprint2": "npx ts-node src/tests/test-sprint2.ts",
    "test:sprint3": "npx ts-node src/tests/test-sprint3.ts",
    "test:all-sprints": "npm run test:sprint1 && npm run test:sprint2 && npm run test:sprint3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@google-cloud/vision": "^5.3.4",
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@prisma/client": "^6.19.0",
    "@types/bull": "^3.15.9",
    "bcryptjs": "^3.0.3",
    "bull": "^4.16.5",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "openai": "^6.9.0",
    "prisma": "^6.19.0",
    "string-similarity": "^4.0.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
